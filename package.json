{
  "name": "vscode-navigate-edit-history",
  "displayName": "Edit History Bookmarks",
  "description": "Navigate your edit history with ease. Jump one step back with a keypress or view a list of all recent edits.",
  "version": "1.6.0",
  "publisher": "codeandstuff",
  "license": "MIT",
  "icon": "logo/icon.png",
  "engines": {
    "vscode": "^1.41.0"
  },
  "categories": [
    "Other"
  ],
  "keywords": [
    "navigate",
    "edit",
    "history",
    "jump",
    "back"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/pgsandstrom/vscode-navigate-edit-history.git"
  },
  "activationEvents": [
    "*"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "configuration": {
      "type": "object",
      "title": "Vscode Navigate Edit History configuration",
      "properties": {
        "navigateEditHistory.maxHistorySize": {
          "type": "number",
          "default": 20,
          "description": "%navigateEditHistory.maxHistorySize%"
        },
        "navigateEditHistory.centerOnReveal": {
          "type": "boolean",
          "default": true,
          "description": "%navigateEditHistory.centerOnReveal%"
        },
        "navigateEditHistory.groupEditsWithinLines": {
          "type": "number",
          "default": 1,
          "description": "%navigateEditHistory.groupEditsWithinLines%"
        },
        "navigateEditHistory.logDebug": {
          "type": "boolean",
          "default": false,
          "description": "%navigateEditHistory.logDebug%"
        },
        "navigateEditHistory.topStackWhenQuickPickSelect": {
          "type": "boolean",
          "default": false,
          "description": "%navigateEditHistory.topStackWhenQuickPickSelect%"
        },
        "navigateEditHistory.topStackWhenMove": {
          "type": "boolean",
          "default": false,
          "description": "%navigateEditHistory.topStackWhenMove%"
        },
        "navigateEditHistory.filterOnPathInEditList": {
          "type": "boolean",
          "default": false,
          "description": "%navigateEditHistory.filterOnPathInEditList%"
        },
        "navigateEditHistory.markerStyle": {
          "type": "string",
          "default": "leftRect",
          "enum": [
            "none",
            "leftRect",
            "leftDash",
            "leftDot",
            "bottomDot",
            "background",
            "SVG rectangle",
            "SVG triangle",
            "custom"
          ],
          "markdownEnumDescriptions": [
            "%navigateEditHistory.markerStyleEnum0%",
            "%navigateEditHistory.markerStyleEnum1%",
            "%navigateEditHistory.markerStyleEnum2%",
            "%navigateEditHistory.markerStyleEnum3%",
            "%navigateEditHistory.markerStyleEnum4%",
            "%navigateEditHistory.markerStyleEnum5%",
            "%navigateEditHistory.markerStyleEnum6%",
            "%navigateEditHistory.markerStyleEnum7%",
            "%navigateEditHistory.markerStyleEnum8%"
          ],
          "markdownDescription": "%navigateEditHistory.markerStyle%"
        },
        "navigateEditHistory.markerColor": {
          "type": "string",
          "default": "#FF00FF99",
          "markdownDescription": "%navigateEditHistory.markerColor%"
        },
        "navigateEditHistory.markerJSON": {
          "type": "object",
          "default": {},
          "readOnly": true,
          "markdownDescription": "%navigateEditHistory.markerJSON%"
        },
        "navigateEditHistory.markerJSONCustom": {
          "type": "object",
          "default": {},
          "markdownDescription": "%navigateEditHistory.markerJSONCustom%"
        },
        "navigateEditHistory.markerWholeLine": {
          "type": "boolean",
          "default": false,
          "markdownDescription": "%navigateEditHistory.markerWholeLine%"
        }
      }
    },
    "commands": [
      {
        "command": "navigateEditHistory.moveCursorToPreviousEdit",
        "title": "Edits: Navigate back in edit history"
      },
      {
        "command": "navigateEditHistory.moveCursorToNextEdit",
        "title": "Edits: Navigate forward in edit history"
      },
      {
        "command": "navigateEditHistory.moveCursorToPreviousEditInCurrentFile",
        "title": "Edits: Navigate back in edit history in the current file"
      },
      {
        "command": "navigateEditHistory.moveCursorToNextEditInCurrentFile",
        "title": "Edits: Navigate forward in edit history in the current file"
      },
      {
        "command": "navigateEditHistory.list",
        "title": "Edits: List edit history in all files"
      },
      {
        "command": "navigateEditHistory.createEditAtCursor",
        "title": "Edits: Create edit at cursor position (Moves edit to top of stack)"
      },
      {
        "command": "navigateEditHistory.removeEditAtCursor",
        "title": "Edits: Remove any edits on the current line"
      },
      {
        "command": "navigateEditHistory.toggleEditAtCursor",
        "title": "Edits: Toggle any edits on the current line"
      },
      {
        "command": "navigateEditHistory.moveCursorCancelNavigation",
        "title": "Edtis: Cancel navigate back in edit history and return to starting position"
      },
      {
        "command": "navigateEditHistory.clearEdits",
        "title": "Edits: Clear All Edits"
      }
    ],
    "keybindings": [
      {
        "command": "navigateEditHistory.moveCursorToPreviousEdit",
        "key": "ctrl+q",
        "mac": "ctrl+q"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "webpack --mode production",
    "webpack": "webpack --mode development",
    "webpack-dev": "webpack --mode development --watch",
    "test-compile": "tsc -p ./",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile",
    "test": "node ./out/test/runTest.js",
    "format": "prettier --write \"{,!(.vscode-test|node_modules|out|package-lock.json|yarn.lock)}{,/**/*}.{json,md,js,jsx,ts,tsx,css,scss}\"",
    "lint": "node ./node_modules/eslint/bin/eslint.js --ext .ts --ext .tsx --fix src",
    "package": "vsce package",
    "pre-commit": "lint-staged",
    "pre-push": "npm run lint && npm run typecheck",
    "prepare": "husky install",
    "typecheck": "tsc --noEmit"
  },
  "lint-staged": {
    "*.{json,md,css,scss}": [
      "prettier --write"
    ],
    "*.{ts,tsx}": [
      "node ./node_modules/eslint/bin/eslint.js --fix",
      "prettier --write"
    ]
  },
  "devDependencies": {
    "@types/glob": "7.2.0",
    "@types/mocha": "9.1.0",
    "@types/node": "14.18.9",
    "@types/vscode": "1.41.0",
    "@typescript-eslint/eslint-plugin": "5.10.0",
    "@typescript-eslint/parser": "5.10.0",
    "cross-env": "7.0.3",
    "eslint": "8.7.0",
    "eslint-config-prettier": "8.3.0",
    "glob": "7.2.0",
    "gulp": "^4.0.2",
    "del": "^6.0.0",
    "gulp-rename": "^2.0.0",
    "gulp-sourcemaps": "^3.0.0",
    "gulp-typescript": "^6.0.0-alpha.1",
    "event-stream": "3.3.5",
    "vscode-nls-dev": "^4.0.0",
    "husky": "7.0.4",
    "lint-staged": "12.3.1",
    "mocha": "9.1.4",
    "prettier": "2.5.1",
    "ts-loader": "9.2.6",
    "typescript": "4.5.5",
    "vsce": "1.96.1",
    "vscode-test": "1.6.1",
    "webpack": "5.67.0",
    "webpack-cli": "4.9.1"
  },
  "dependencies": {}
}
